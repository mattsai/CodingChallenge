    "def mySqrt(self, x: int) -> int:\n",
    "        if x ==0 :return 0\n",
    "        est = x / 2.0\n",
    "\n",
    "        for _ in range(1000):\n",
    "            est= (est + x / est) / 2.0\n",
    "\n",
    "        return int(est)\n",
    "        "
